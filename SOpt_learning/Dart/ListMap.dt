import 'dart:io';
 
class Idade {
    int idade;
    Idade(this.idade);
}

void main() {
    Map<String, Idade> pessoas = Map();
    pessoas['Fulano'] = Idade(48);
    pessoas['Ciclano'] = Idade(19);
    print(pessoas.keys);
    print(pessoas.values);
    for (var pessoa in pessoas.values) {
        print(pessoa.idade);
    }
}

